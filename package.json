{
  "name": "@jimengio/jimo-antd-theme",
  "version": "0.1.0",
  "description": "Antd theme for jimengio",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "example": "parcel ./example/index.html",
    "example:build": "rm -rf ./dist/ && parcel build ./example/index.html --out-dir dist --public-url ./",
    "example:deploy": "yarn example:build && gh-pages -d dist",
    "build": "rm -rf ./lib/ && tsc -d --project tsconfig.json --outDir ./lib/",
    "prepare": "yarn build"
  },
  "keywords": [
    "antd-theme",
    "antd",
    "less"
  ],
  "author": "JimengIo",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "emotion": "^10.0.23",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.2"
  }
}
